{
  "name": "http-downloader",
  "version": "1.0.0",
  "description": "A downloader for http and parser for video streaming websites.",
  "main": "dev/main/main.js",
  "scripts": {
    "copy-files-dev": "copyfiles -u 1 src/renderer/index.html dev/ && copyfiles resources/favicon.ico dev/",
    "copy-files-prod": "copyfiles -u 1 src/renderer/index.html build/app/ && copyfiles resources/favicon.ico build/app/",
    "make-main-dev": "tsc --build tsconfig.dev.main.json",
    "make-main-prod": "tsc --build tsconfig.prod.main.json",
    "make-renderer-dev": "webpack --config webpack.config.js && yarn run copy-files-dev",
    "make-renderer-prod": "webpack --config webpack.prod.config.js && yarn run copy-files-prod",
    "dev": "yarn install && yarn run make-main-dev && yarn run make-renderer-dev && yarn install && cross-env NODE_ENV=development electron --enable-logging --inspect --start-mode=normal .",
    "build": "yarn install && node scripts/build.js"
  },
  "author": "hzh",
  "license": "ISC",
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "fluent-ffmpeg": "^2.1.2",
    "get-proxy-settings": "^0.1.13",
    "html-escaper": "^3.0.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3",
    "sqlite3": "^5.1.4",
    "utimes": "^5.1.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/html-escaper": "^3.0.0",
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "adm-zip": "^0.5.10",
    "babel-loader": "^8.2.5",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "^19.0.8",
    "file-loader": "^6.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shelljs": "^0.8.5",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
