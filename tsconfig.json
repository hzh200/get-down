{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ESNext",
    "rootDir": "src/",
    "outDir": "dist/",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "removeComments": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "moduleResolution": "node",
    "jsx": "react",
    "allowJs": false,
    "noEmitOnError": true
  },
  // "include": ["**/*.ts", "**/*.tsx", "src/main/main.ts"]
  // "include": ["./images.d.ts", "src/main/*.ts", "src//*.ts"]
  "include": ["src/"]
}